{
  "name": "@ttbs/api",
  "version": "0.0.1",
  "description": "Backend for Iyyara merchant, Express application with development tools pre-configured",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "coverage": "dotenv -e .env.test -- cross-env NODE_ENV=test jest test/**/*.spec.ts --coverage",
    "dev": "dotenv -e .env -- cross-env NODE_ENV=development tsx watch src/index.ts",
    "prod": "dotenv -e .env.prod -- pnpm build && cross-env NODE_ENV=production node -r tsconfig-paths/register dist/src/index.js",
    "type-check": "tsc --pretty --noEmit",
    "type-check:ci": "tsc-absolute --pretty --noEmit",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "lint:report": "eslint . --format json --output-file ../../lint-results/api.json"
  },
  "author": "TISOHA",
  "license": "ISC",
  "dependencies": {
    "@t3-oss/env-core": "^0.7.1",
    "@ttbs/lib": "workspace:*",
    "@ttbs/prisma": "workspace:*",
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "express-winston": "^4.2.0",
    "handlebars": "^4.7.7",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "logform": "^2.5.1",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "nodemailer-mailgun-transport": "^2.1.5",
    "qs": "^6.11.2",
    "redis": "^4.6.7",
    "safe-stable-stringify": "^2.4.3",
    "sharp": "^0.32.5",
    "socket.io": "^4.7.1",
    "stripe": "^13.10.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "triple-beam": "^1.4.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@ttbs/env": "workspace:*",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^20.8.1",
    "@types/nodemailer": "^6.4.9",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/triple-beam": "^1.3.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/ui": "^0.33.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.2.3",
    "openapi-types": "^12.1.3",
    "replace": "^1.2.2",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.1",
    "typescript": "^5.2.2",
    "vitest": "^0.33.0",
    "vitest-mock-extended": "^1.1.4"
  }
}
